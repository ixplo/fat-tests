{
  "request": {
    "method": "ANY",
    "urlPattern": "/api/rate-limited"
  },
  "response": {
    "status": 429,
    "headers": {
      "Content-Type": "application/json",
      "Retry-After": "60",
      "X-RateLimit-Limit": "100",
      "X-RateLimit-Remaining": "0",
      "X-RateLimit-Reset": "1609459200"
    },
    "jsonBody": {
      "error": "Too Many Requests",
      "message": "Rate limit exceeded. Try again later.",
      "code": "RATE_LIMITED"
    }
  }
}
