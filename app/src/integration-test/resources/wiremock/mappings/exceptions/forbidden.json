{
  "request": {
    "method": "DELETE",
    "urlPattern": "/api/admin/.*",
    "headers": {
      "X-Role": {
        "doesNotMatch": "ADMIN"
      }
    }
  },
  "response": {
    "status": 403,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "error": "Forbidden",
      "message": "Admin role required for this operation",
      "code": "ACCESS_DENIED"
    }
  }
}
